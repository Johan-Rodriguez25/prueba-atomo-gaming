{
  "name": "prueba_atomo_gaming",
  "version": "0.1.0",
  "description": "Backend de prueba tecnica para atomo gaming",
  "main": "./dist/app.js",
  "scripts": {
    "test:dev": "cross-env NODE_ENV=development npx jest --watch",
    "test:prod": "cross-env NODE_ENV=production npx jest --watch",
    "test:coverage": "jest --coverage -c ./jest.config.json",
    "lint": "eslint --ext .ts src/**",
    "build": "tsc",
    "build:docker": "docker build -t rocketfy/microservice .",
    "docs": "typedoc",
    "start": "cross-env NODE_ENV=production tsc-watch --project . --outDir ./dist --onSuccess \"node ./dist/app.js\"",
    "stage": "cross-env NODE_ENV=development tsc-watch --project . --outDir ./dist --onSuccess \"node ./dist/app.js\"",
    "prod": "cross-env NODE_ENV=production tsc-watch --project . --outDir ./dist --onSuccess \"nodemon ./dist/app.js\"",
    "dev": "cross-env NODE_ENV=development tsc-watch --project . --outDir ./dist --onSuccess \"nodemon ./dist/app.js\"",
    "local": "cross-env NODE_ENV=local tsc-watch --project . --outDir ./dist --onSuccess \"nodemon ./dist/app.js\""
  },
  "keywords": [],
  "author": "Johan Rodriguez",
  "license": "MIT",
  "engines": {
    "node": ">=20.18.0"
  },
  "packageManager": "yarn@4.5.0",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "apicache": "^1.6.3",
    "better-queue": "^3.8.12",
    "bson": "^6.8.0",
    "cloudinary": "^2.5.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-flow": "^4.1.0",
    "express": "^4.21.0",
    "express-fileupload": "^1.5.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "mongodb": "^6.9.0",
    "morgan": "^1.10.0",
    "rxjs": "^7.8.1",
    "tsc-watch": "^6.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/apicache": "^1.6.6",
    "@types/better-queue": "^3.8.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv-flow": "^3.3.3",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "husky": "^9.1.6",
    "nodemon": "^3.1.7",
    "typescript": "^5.6.2"
  }
}
